{
    "presets": [
        ["@babel/preset-env",
        {
          "targets": {
            "chrome": "70",
            "ie": "11",
            "safari": "10",
            "firefox": "60"
          }
        }],
        ["@babel/preset-react", {
          "runtime": "automatic"
        }]    
      ],
      "plugins": [
        "@babel/plugin-syntax-dynamic-import",
        "@babel/plugin-proposal-class-properties",
        "react-hot-loader/babel"
      ]
  }